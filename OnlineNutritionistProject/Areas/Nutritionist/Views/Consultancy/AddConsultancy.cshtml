@model Consultancy
@{
    ViewData["Title"] = "AddConsultancy";
    Layout = "~/Views/Shared/_NutritionistLayout.cshtml";
}

<h1>AddConsultancy</h1>

<div class="content">
    <div class="row">
        <div class="col-md-12">
            <div class="card">
                <div class="card-header">
                    <h4 class="card-title">Kitaplarınız</h4>
                </div>

                <div class="col-md-8 col-sm-7">
                    <form method="post" asp-controller="Consultancy">
                        <div class="blog-content wow fadeInUp" data-wow-delay="1s">




                           
                            <h3>
                                <div class="form-group">
                                    @Html.Label("Başlık")
                                    <input type="text" asp-for="Description" class="form-control" />
                                </div>
                            </h3>
                         




                            <div class="form-group">

                                <input type="text" asp-for="AppUserId" class="form-control" value="@ViewBag.userId" />
                            </div>
                            <button class="btn btn-outline-primary">Ekle</button>
                        </div>
                    </form>



                </div>
            </div>
        </div>
    </div>
</div>